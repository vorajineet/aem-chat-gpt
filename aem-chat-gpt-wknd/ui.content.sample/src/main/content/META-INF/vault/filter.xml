<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">

    <!-- Content Filters -->
    <filter root="/content/wknd" />
    <!-- Changing the filter mode to merge will preserve any existing content on the target environment
    <filter root="/content/wknd" mode="merge"/>
    -->
    <filter root="/content/dam/wknd" mode="update"/>
    <filter root="/content/experience-fragments/wknd"/>

    <!-- Force Policy and Template Updates -->
    <filter root="/conf/wknd/settings/wcm" />
    <filter root="/conf/wknd/settings/cloudconfigs" mode="merge" />
    <filter root="/conf/wknd/settings/cloudsettings" mode="merge" />

    <!-- WKND Member Group-->
    <filter root="/home/groups/wknd" mode="merge" />
    <!-- Sample User-->
    <filter root="/home/users/wknd" />
</workspaceFilter>
